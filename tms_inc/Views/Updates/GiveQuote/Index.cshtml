@model IEnumerable<tms_inc.Models.RequestedQuote.RequestedQuote>

@{
    Layout = "_Layout";
}
<h3>Quote requests</h3>

<nav class="nav nav-pills flex-column flex-sm-row">
    <a class="flex-sm-fill text-sm-center nav-link btn btn-outline-info active" id="pendingQuoteRequests" aria-current="page"><i class="fa fa-spinner" aria-hidden="true"> pending</i></a>
    <a class="flex-sm-fill text-sm-center nav-link btn btn-outline-info" id="processedQuoteRequests"><i class="fa fa-check-square-o" aria-hidden="true"> processed</i></a>
    <a class="flex-sm-fill text-sm-center nav-link btn btn-outline-info" id="deletedQuoteRequests"><i class="fa fa-archive" aria-hidden="true"> deleted</i></a>
</nav>

<div id="result">
    @await Html.PartialAsync("GiveQuote/Table", Model)

</div>